@model WebLib.Models.LibrarianModel

    <div class="row">
        <ul class="list-unstyled user-card-list">
            <li class="user-card-li text-center">
                <h4>@Model.FullName</h4>
            </li>
            <li class="user-card-li text-center">
                <small class="text-muted"> @Model.LibraryName</small>
            </li>
            <li class="user-card-li text-center">
                <small class="text-muted"> @Model.Phone</small>
            </li>
        </ul>
    </div>
